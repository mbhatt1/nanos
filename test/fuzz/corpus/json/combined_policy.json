{
  "version": "1.0",
  "fs": {
    "read": ["/etc/**", "/lib/**", "/lib64/**", "/proc/self/**"],
    "write": ["/tmp/**", "/var/log/**"]
  },
  "net": {
    "dns": ["*.example.com", "api.github.com"],
    "connect": ["ip:*:443", "ip:*:80"]
  },
  "tools": {
    "allow": ["bash", "python", "curl"],
    "deny": ["rm", "dd"]
  },
  "budgets": {
    "tool_calls": 100,
    "tokens": 50000,
    "wall_time_ms": 300000
  }
}